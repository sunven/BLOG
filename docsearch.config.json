{
  "index_name": "blog",
  "start_urls": ["https://llweb.top"],
  "stop_urls": [],
  "selectors": {
    "lvl0": {
      "selector": ".VPLink.link.active",
      "global": true,
      "default_value": "Documentation"
    },
    "lvl1": ".content-container h1",
    "lvl2": ".content-container h2",
    "lvl3": ".content-container h3",
    "lvl4": ".content-container h4",
    "lvl5": ".content-container h5",
    "text": ".content-container p, .content-container li",
    "lang": {
      "selector": "/html/@lang",
      "type": "xpath",
      "global": true
    }
  },
  "custom_settings": {
    "attributesForFaceting": ["lang"]
  }
}
